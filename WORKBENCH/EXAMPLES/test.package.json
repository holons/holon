{
  "name": "LeadShareApp",
  "version": "0.0.2",
  "description": "[APP] - PaaS for sharing costs for google ad campaigns to get leads",
  "directories": {
    "test": "SPECIFICATION"
  },
  "scripts": {
    "todo": "http://browsenpm.org/package.json",
    "todo2": "https://www.npmjs.org/doc/files/package.json.html",
    "help": "#describe all npm tasks - http://anders.janmyr.com/2014/03/running-scripts-with-npm.html + http://substack.net/task_automation_with_npm_run",
    "jobs": "#list jobs + description on how to start working on one + how to stop development",
    "develop": "#checkout job branch + stylus & watch & testem + how to stop development",
    "deploy": "#build + run all tests + git push if successful AND ... what about docker???",
    "release": "#build through browserify, test all + if successful: uglify + semver + push with tags + NOT npm publish, bower register, ....",
    "test": "echo \"Error: no test specified\" && exit 1 #run testem",
    "spec": "npm run test",
    "uglify": "#https://github.com/hughsk/uglifyify",
    "stuff": "#echo v$(cat package.json | grep version | grep -Po '(?<=version\": \").*(?=\")'), npm run serve, npm run spec",

    "start": "node_modules/atomify/bin/atomify.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ninabreznik/LeadShareApp.git"
  },
  "keywords": [
    "lead",
    "leads",
    "campaign",
    "campaign",
    "google",
    "ads",
    "adwords"
  ],
  "atomify": {
    "server": {
      "port": 1337,
      "url": "http://localhost:3000/",
      "lr": {
        "verbose": true,
        "quiet": false,
        "patterns": ["app/assets/stylesheets/APP.bundle.css"],
        "port": 31337,
        "sync": true
      }
    },
    "js": {
      "entry": "app/assets/javascripts/index.js",
      "alias": "/assets/APP.bundle.js",
      "output": "app/assets/javascripts/APP.bundle.js",
      "debug": true,
      "transforms": []
    },
    "css": {
      "entry": "app/assets/stylesheets/index.css",
      "alias": "/assets/APP.bundle.css",
      "output": "app/assets/stylesheets/APP.bundle.css",
      "debug": true,
      "watch": true,
      "compress": true,
      "plugin": [],
      "variables": {}
    },
    "assets": {
      "dest": "app/assets/images",
      "prefix": "../assets/",
      "retainName": false
    }
  },
  "engines": {
    "node": ">= 0.10.25"
  },
  "author": [
    "serapath <dev@serapath.de> (http://www.github.com/serapath)",
    "nin <ninabreznik@gmail.com> (http://www.github.com/ninabreznik)"
  ],
  "licenses": [{
    "type": "CC BY-NC-ND 4.0",
    "url": "https://creativecommons.org/licenses/by-nc-nd/4.0/"
  }],
  "license": "CC BY-NC-ND 4.0",
  "bugs": {
    "url": "https://github.com/ninabreznik/LeadShareApp/issues"
  },
  "homepage": "https://github.com/ninabreznik/LeadShareApp",
  "private": true,
  "dependencies": {
    "LeadShareAppFrontend": "git://github.com/coding-amigos/LeadShareAppFrontend#v0.0.8"
  },
  "devDependencies": {
    "atomify": "git://github.com/serapath/atomify"
  },
  "optionalDependencies": {},
  "peerDependencies": {}
}
